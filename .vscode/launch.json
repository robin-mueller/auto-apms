{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch new_tree",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder:px4-ros2-env}/install/auto_apms_behavior_tree/lib/auto_apms_behavior_tree/new_tree",
            "args": ["${workspaceFolder:px4-ros2-env}/text.xml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder:px4-ros2-env}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "TreeExecutor debugger",
            "type": "cppdbg",
            "request": "launch",
            "miDebuggerServerAddress": "localhost:3000",
            "cwd": "/",
            "program": "${workspaceFolder:px4-ros2-env}/install/auto_apms_behavior_tree/lib/auto_apms_behavior_tree/tree_executor"
        }
    ]
}