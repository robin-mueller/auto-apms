auto_apms_behavior_tree::executor_params:
  ${_AUTO_APMS_BEHAVIOR_TREE__EXECUTOR_PARAM_TREE_CREATOR}:
    type: string
    default_value: auto_apms_behavior_tree::ResourceTreeCreator
    description: Fully qualified class name of the behavior tree builder responsible for creating trees by default.
  ${_AUTO_APMS_BEHAVIOR_TREE__EXECUTOR_PARAM_TICK_RATE}:
    type: double
    default_value: 0.1
    description: Rate [s] at which the behavior tree is being ticked.
    validation:
      gt_eq<>: [0]

  ${_AUTO_APMS_BEHAVIOR_TREE__EXECUTOR_PARAM_GROOT2_PORT}:
    type: int
    default_value: 5555
    description: Server port for the Groot2 publisher.
    validation:
      gt_eq<>: [0]

  ${_AUTO_APMS_BEHAVIOR_TREE__EXECUTOR_PARAM_STATE_CHANGE_LOGGER}:
    type: bool
    default_value: false
    description: Flag whether to write any behavior tree state changes to rosout.
