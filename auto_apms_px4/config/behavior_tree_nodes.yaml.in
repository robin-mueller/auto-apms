Arm:
  class_name: auto_apms_px4::ArmAction
  port: ${_AUTO_APMS_PX4__ARM_DISARM_ACTION_NAME}
  logger_level: DEBUG

Disarm:
  class_name: auto_apms_px4::DisarmAction
  port: ${_AUTO_APMS_PX4__ARM_DISARM_ACTION_NAME}
  logger_level: DEBUG

EnableHold:
  class_name: auto_apms_px4::EnableHoldAction
  port: ${_AUTO_APMS_PX4__ENABLE_HOLD_ACTION_NAME}
  logger_level: DEBUG

GoTo:
  class_name: auto_apms_px4::GoToAction
  port: ${_AUTO_APMS_PX4__GOTO_ACTION_PREFIX}(input:frame)
  logger_level: DEBUG

Land:
  class_name: auto_apms_px4::LandAction
  port: ${_AUTO_APMS_PX4__LAND_ACTION_NAME}
  logger_level: DEBUG

Mission:
  class_name: auto_apms_px4::MissionAction
  port: ${_AUTO_APMS_PX4__MISSION_ACTION_NAME}
  logger_level: DEBUG

RTL:
  class_name: auto_apms_px4::RTLAction
  port: ${_AUTO_APMS_PX4__RTL_ACTION_NAME}
  logger_level: DEBUG

Takeoff:
  class_name: auto_apms_px4::TakeoffAction
  port: ${_AUTO_APMS_PX4__TAKEOFF_ACTION_NAME}
  logger_level: DEBUG

GetGlobalPosition:
  class_name: auto_apms_px4::GetPosition<GlobalPositionMsg>
  port: /fmu/out/vehicle_global_position
  logger_level: DEBUG

GetLocalPosition:
  class_name: auto_apms_px4::GetPosition<LocalPositionMsg>
  port: /fmu/out/vehicle_local_position
  logger_level: DEBUG
